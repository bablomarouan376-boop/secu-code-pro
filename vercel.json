{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    { "src": "/manifest.json", "dest": "/static/manifest.json" },
    { "src": "/sitemap.xml", "dest": "/static/sitemap.xml" },
    { "src": "/robots.txt", "dest": "/static/robots.txt" },
    { "src": "/(.*)", "dest": "app.py" }
  ]
}
